<Alloy>
	<Collection src="Catagory">
	<ListView>
		<Templates>
			
			<ItemTemplate name="catagoryName">
				<Label bindId='title'></Label>
				<ImageView bindId='image'></ImageView>
			</ItemTemplate>
			
			</Templates>
			<ListSection dataCollection='Catagory'>
				<ListItem title:text="name" image:image="image" template="catagoryName"></ListItem>
			</ListSection>
	</ListView>
</Alloy>